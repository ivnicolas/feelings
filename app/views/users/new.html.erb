<h1> You have hit the new users page <h1>
<%= form_with model: @user, local: true do |f| %>

    <%= f.label :name, "Enter Your Name" %>
    <%= f.text_field :name , placeholder: "Name Here" %>

    <%= f.label :email, "Enter Your Email" %>
    <%= f.text_field :email , placeholder: "Register with Email" %>

    <%= f.label :password, "Please Enter a Password " %>
    <%= f.password_field :password %>
    <%= f.submit "Submit" %>
<% end %>