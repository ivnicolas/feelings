 <div class="form">
<div class="new-user">
 <%= form_for :user do |f| %>
    <%= f.label :email, "Enter Your Email:" %>
    <%= f.email_field :email , placeholder: "Email" %>
    <br>
    <%= f.label :password, "Enter Your Password:" %>
    <%= f.password_field :password , placeholder: "Password" %>
    <br>
    <%= f.label :name, "Name:" %>
    <%= f.text_field :name , placeholder: "Name Here" %>
    <br>
    <%= f.label :age, "Age:" %>
    <%= f.number_field :age , placeholder: "Age" %>
    <br>
    <%= f.label :zipcode, "Zipcode:" %>
    <%= f.number_field :zipcode , placeholder: "Zipcode"%>
    <br>
    <%= f.label :goal, "What Do You Hope To Achieve By Using this Application?" %>
    <br>
    <%= f.text_area :goal %>
    <br>
    <%= f.label :mantra, "What is Your Mantra?" %>
    <br>
    <%= f.text_area :mantra %>
    <br>
    <%= f.submit "SUBMIT" , class: 'input button-2'%>
<% end %>
</div>
</div>